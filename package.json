{
  "name": "Arek-starter",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "serve": "./node_modules/.bin/http-server -p 8080",
    "build": "webpack",
    "postinstall": "webpack --config webpack.prod.config.js -p",
    "start": "node main.js",
    "test": "xo"
  },
  "author": "Arkadiusz Zalewski",
  "license": "ISC",
  "engines": {
    "node": "6.4.0"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.4",
    "express-graphql": "^0.6.11",
    "file-loader": "^0.11.1",
    "graphql": "^0.11.3",
    "http-server": "^0.9.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-dom-confetti": "^0.0.7",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.5.2",
    "sqlite3": "^3.1.10",
    "style-loader": "^0.18.2",
    "styled-components": "^1.4.5",
    "svg-sprite-loader": "^3.0.3",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.12.0",
    "eslint-plugin-react": "^7.0.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "semicolon": false,
    "extends": "xo-react",
    "rules": {
      "import/first": 1,
      "import/newline-after-import": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/order": 0,
      "no-mixed-operators": 1,
      "react/jsx-filename-extension": 0,
      "react/jsx-tag-spacing": 1,
      "react/require-optimization": 1,
      "react/require-default-props": 0,
      "react/prop-types": 1,
      "unicorn/no-abusive-eslint-disable": 1,
      "unicorn/filename-case": 0,
      "unicorn/explicit-length-check": 0,
      "max-statements-per-line": [
        2,
        {
          "max": 2
        }
      ],
      "quotes": 0,
      "quote-props": 0,
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-eq-null": 0,
      "operator-linebreak": [
        2,
        "after",
        {
          "overrides": {
            "?": "ignore",
            ":": "ignore"
          }
        }
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "babel/arrow-parens": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "react/jsx-closing-bracket-location": [
        2,
        "tag-aligned"
      ],
      "react/jsx-no-bind": 0,
      "import-order/import-order": 0,
      "prefer-template": 0
    }
  }
}
